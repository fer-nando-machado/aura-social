{
  "name": "aura-social",
  "homepage": "http://fernandomachado90.github.io/aura-social",
  "version": "2.0.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "colorthief": "^2.3.2",
    "conic-gradient": "^1.0.0",
    "node-sass": "^4.14.1",
    "prefixfree": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "format": "prettier --write --no-semi --trailing-comma es5 --print-width 120 '**/*.js'",
    "start": "HTTPS=true react-scripts start",
    "bundle": "cp node_modules/prefixfree/prefixfree.min.js node_modules/conic-gradient/conic-gradient.js public/scripts/",
    "build": "npm run bundle && react-scripts build",
    "test": "react-scripts test --env jest-environment-jsdom-fourteen",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "gh-pages": "^3.1.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.0.5"
  }
}
